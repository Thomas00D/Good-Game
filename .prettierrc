{
  "importOrder": [
    "^(react|react-dom)$",
    "^next",
    "<THIRD_PARTY_MODULES>",
    "^@/components/",
    "^@/",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ]
}
